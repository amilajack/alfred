{
  "name": "alfred-cli",
  "version": "0.0.1-10",
  "description": "A framework for upgrading javascript codebases",
  "bin": {
    "alfred": "./lib/alfred.js",
    "alfred-cli": "./lib/alfred.js"
  },
  "preferGlobal": true,
  "repository": "git@github.com:amilajack/alfred.git",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-babel": "cross-env NODE_ENV=production babel src --out-dir lib && cp -r src/providers/resources ./lib/providers",
    "build-webpack": "webpack -p --config webpack.config.js",
    "build": "yarn build-babel",
    "flow": "flow",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run --silent lint -- --fix; exit 0",
    "package": "pkg ./lib/alfred.js --out-path release",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.js'",
    "spec": "jest",
    "test": "cross-env NODE_ENV=test yarn build-babel && yarn spec",
    "version": "rm -rf lib && yarn build",
    "watch": "cross-env NODE_ENV=production babel src --out-dir lib --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-formatter-pretty": "^1.3.0",
    "flow-bin": "^0.81.0",
    "jest-cli": "^23.6.0",
    "pkg": "^4.3.4",
    "shebang-loader": "^0.0.1",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "dependencies": {
    "5to6-codemod": "^1.7.1",
    "child-process-promise": "^2.2.1",
    "commander": "^2.18.0",
    "eslint": "^5.6.0",
    "eslint-config-bliss": "^2.3.0",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "findit": "^2.0.0",
    "gitignore-parser": "^0.0.2",
    "jscodeshift": "^0.5.1",
    "lebab": "^3.0.2",
    "lodash.flatten": "^4.4.0",
    "prettier": "^1.14.2",
    "rndm": "^1.2.0",
    "simple-git": "^1.96.0",
    "uniq": "^1.0.1"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "files": [
    "lib"
  ]
}
