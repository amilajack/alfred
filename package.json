{
  "name": "alfred-cli",
  "version": "0.0.1-5",
  "description": "A framework for upgrading javascript codebases",
  "bin": {
    "alfred": "./lib/alfred.js",
    "alfred-cli": "./lib/alfred.js"
  },
  "preferGlobal": true,
  "repository": "git@github.com:amilajack/alfred.git",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-babel": "cross-env NODE_ENV=production babel src --out-dir lib && cp -r src/providers/resources ./lib/providers",
    "build-webpack": "webpack -p --config webpack.config.js",
    "build": "yarn build-babel",
    "flow": "flow",
    "lint": "eslint .",
    "package": "pkg ./lib/alfred.js --out-path release",
    "spec": "jest",
    "test": "cross-env NODE_ENV=test yarn spec",
    "version": "rm -rf lib && yarn build",
    "watch": "cross-env NODE_ENV=production babel src --out-dir lib --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "flow-bin": "^0.58.0",
    "jest-cli": "^21.2.1",
    "pkg": "^4.2.6",
    "shebang-loader": "^0.0.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "dependencies": {
    "5to6-codemod": "^1.7.1",
    "child-process-promise": "^2.2.1",
    "commander": "^2.11.0",
    "eslint": "^4.10.0",
    "eslint-config-bliss": "^1.0.19",
    "eslint-plugin-flowtype-errors": "^3.3.5",
    "findit": "^2.0.0",
    "gitignore-parser": "^0.0.2",
    "jscodeshift": "^0.3.32",
    "lebab": "^2.7.7",
    "lodash.flatten": "^4.4.0",
    "prettier": "^1.7.4",
    "rndm": "^1.2.0",
    "simple-git": "^1.80.1",
    "uniq": "^1.0.1"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "files": [
    "lib"
  ]
}
