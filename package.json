{
  "name": "alfred-cli",
  "version": "0.0.1-12",
  "description": "A framework for upgrading javascript codebases",
  "bin": {
    "alf": "./lib/alfred.js",
    "alfred": "./lib/alfred.js",
    "alfred-cli": "./lib/alfred.js"
  },
  "preferGlobal": true,
  "repository": "git@github.com:amilajack/alfred.git",
  "author": "Amila Welihinda <amilajack@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib && cp -r src/providers/resources ./lib/providers",
    "build-watch": "cross-env NODE_ENV=production babel src --out-dir lib --watch",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "postinstall": "yarn flow-typed",
    "spec": "jest",
    "test": "cross-env NODE_ENV=test yarn build && yarn lint && yarn flow && yarn spec",
    "version": "rm -rf lib && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint-formatter-pretty": "^2.0.0",
    "espree": "^5.0.0",
    "flow-bin": "^0.89.0",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "jest-cli": "^23.6.0",
    "lint-staged": "^8.1.0",
    "shebang-loader": "^0.0.1"
  },
  "dependencies": {
    "5to6-codemod": "^1.7.1",
    "@amilajack/powerset": "^0.0.0",
    "child-process-promise": "^2.2.1",
    "commander": "^2.19.0",
    "eslint": "^5.11.1",
    "eslint-config-bliss": "^3.1.2",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "findit": "^2.0.0",
    "gitignore-parser": "^0.0.2",
    "jscodeshift": "^0.6.2",
    "lebab": "^3.0.3",
    "lodash": "^4.17.11",
    "lodash.flatten": "^4.4.0",
    "rndm": "^1.2.0",
    "simple-git": "^1.107.0",
    "uniq": "^1.0.1",
    "update-notifier": "^2.5.0"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "yarn": ">=1.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "files": [
    "lib"
  ],
  "eslintConfig": {
    "extends": "bliss",
    "rules": {
      "class-methods-use-this": "off"
    }
  },
  "renovate": {
    "extends": "bliss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
