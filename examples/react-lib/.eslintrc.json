{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "bliss",
    "eslint-config-airbnb",
    "prettier"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier",
    "jest"
  ],
  "root": true,
  "rules": {
    "flowtype-errors/show-errors": "off",
    "react/state-in-constructor": "off"
  }
}
