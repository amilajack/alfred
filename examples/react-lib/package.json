{
  "name": "@examples/react-lib",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "node ../../packages/cli/lib/commands/alfred.js run build",
    "clean": "node ../../packages/cli/lib/commands/alfred.js run clean",
    "format": "node ../../packages/cli/lib/commands/alfred.js run format",
    "lint": "node ../../packages/cli/lib/commands/alfred.js run lint",
    "skills": "node ../../packages/cli/lib/commands/alfred.js skills",
    "start": "node ../../packages/cli/lib/commands/alfred.js run start",
    "test": "node ../../packages/cli/lib/commands/alfred.js run test",
    "test-all": "yarn clean && yarn build && yarn lint && yarn format && yarn skills && yarn test"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react-hot-loader": "^4.3.12"
  },
  "devDependencies": {
    "@alfred/cli": "^0.2.0",
    "@alfred/core": "^0.2.0",
    "@alfred/skill-react": "^0.2.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "^7.8.6",
    "@examples/alfred-config-example": "^0.0.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
    "eslint": "~7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.0.1",
    "lodash-es": "4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "parcel": "^1.11.0",
    "postcss-modules": "^2.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hot-loader": "^4.6.3",
    "rollup": "2.17.1",
    "rollup-plugin-babel": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "plugins": [
          "babel-plugin-lodash",
          "babel-plugin-dev-expression",
          "@babel/plugin-transform-react-constant-elements",
          "@babel/plugin-transform-react-inline-elements",
          "babel-plugin-transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": "auto"
            }
          ]
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ],
      "@babel/preset-react"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "bliss",
      "airbnb",
      "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "jest",
      "prettier"
    ],
    "root": true,
    "rules": {
      "react/jsx-filename-extension": "off",
      "react/state-in-constructor": "off"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "postcss": {
    "alias": "postcss",
    "config": {
      "modules": true,
      "plugins": {
        "autoprefixer": {
          "grid": true
        }
      }
    },
    "fileType": "json",
    "filename": ".postcssrc",
    "pkgProperty": "postcss",
    "write": true
  },
  "alfred": {
    "extends": "@examples/alfred-config-example",
    "npmClient": "yarn",
    "skills": [
      "@alfred/skill-react",
      [
        "@alfred/skill-eslint",
        {
          "rules": {
            "react/jsx-filename-extension": "off",
            "react/state-in-constructor": "off"
          }
        }
      ]
    ]
  }
}
