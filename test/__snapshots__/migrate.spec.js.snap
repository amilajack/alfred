/* jshint asi:true */

import test from 'assertit';
import express from 'express';
import request from 'supertest';
import helloWorld from './index';

test('express-hello-world:', () => {
  test('should say \\"Hello World\\"', done => {
    const app = express()
    app.use(helloWorld())

    request(app)
      .get('/')
      .expect(200, 'Hello World')
      .end(done)
  })
  test('should yield next middleware', done => {
    const app = express()
    app
      .use((req, res, next) => {
        res.set('X-First', 'foo')
        next()
      })
      .use(helloWorld())
      .use((req, res, next) => {
        test.equal(res.get('X-First'), 'foo')
      })

    request(app)
      .get('/')
      .expect(200, 'Hello World')
      .expect('X-First', 'foo')
      .end(err => {
        test.ifError(err)
        done()
      })
  })
})
",
