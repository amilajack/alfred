{
  "name": "@fixtures/extend-config",
  "version": "0.2.0-alpha.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "node ../../../../cli/lib/commands/alfred.js run build",
    "clean": "node ../../../../cli/lib/commands/alfred.js run clean",
    "format": "node ../../../../cli/lib/commands/alfred.js run format",
    "lint": "node ../../../../cli/lib/commands/alfred.js run lint",
    "skills": "node ../../../../cli/lib/commands/alfred.js skills",
    "start": "node ../../../../cli/lib/commands/alfred.js run start",
    "test": "node ../../../../cli/lib/commands/alfred.js run test",
    "test-all": "yarn clean && yarn build && yarn lint && yarn format && yarn skills && yarn test"
  },
  "devDependencies": {
    "@alfred/cli": "^0.2.0-alpha.0",
    "@alfred/core": "^0.2.0-alpha.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "@examples/alfred-config-example": "^0.2.0-alpha.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.1.0",
    "lodash-es": "4.17.15",
    "lodash-webpack-plugin": "0.11.5",
    "parcel": "^1.11.0",
    "postcss-modules": "^1.5.0",
    "prettier": "^1.16.4"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "babel-plugin-lodash"
        ]
      }
    },
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "bliss",
      "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "jest",
      "prettier"
    ],
    "root": true
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "grid": true
      }
    }
  },
  "alfred": {
    "configsDir": ".configs",
    "extends": "@examples/alfred-config-example",
    "npmClient": "yarn",
    "showConfigs": true
  }
}
