{
  "name": "@fixtures/library",
  "version": "0.2.0",
  "private": true,
  "license": "MIT",
  "main": "./targets/lib.node.js",
  "module": "./targets/lib.node.js",
  "scripts": {
    "build": "node ../../../../cli/lib/commands/alfred.js run build",
    "clean": "node ../../../../cli/lib/commands/alfred.js run clean",
    "format": "node ../../../../cli/lib/commands/alfred.js run format",
    "lint": "node ../../../../cli/lib/commands/alfred.js run lint",
    "skills": "node ../../../../cli/lib/commands/alfred.js skills",
    "start": "node ../../../../cli/lib/commands/alfred.js run start",
    "test": "node ../../../../cli/lib/commands/alfred.js run test",
    "test-all": "yarn clean && yarn build && yarn lint && yarn format && yarn skills && yarn test"
  },
  "devDependencies": {
    "@alfred/cli": "^0.2.0",
    "@alfred/core": "^0.2.0",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "eslint": "~7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^26.0.1",
    "parcel": "^1.11.0",
    "postcss-modules": "^2.0.0",
    "prettier": "^2.0.5",
    "rollup": "2.17.1",
    "rollup-plugin-babel": "^4.3.2"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": "auto"
            }
          ]
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false
        }
      ]
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "bliss",
      "prettier"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "jest",
      "prettier"
    ],
    "root": true
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "postcss": {
    "modules": true,
    "plugins": {
      "autoprefixer": {
        "grid": true
      }
    }
  },
  "alfred": {
    "configsDir": ".configs",
    "npmClient": "yarn"
  }
}
